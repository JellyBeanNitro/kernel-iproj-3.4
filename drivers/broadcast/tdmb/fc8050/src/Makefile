#
# Makefile for the TDMB FC8050 drivers.
#

# Each configuration option enables a list of files.
# LGE
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= broadcast_fc8050_drv_if.o
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= broadcast_fc8050.o
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= tdmb_tunerbbdrv_fc8050.o
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= bbm.o
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= fc8050_bb.o
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= fc8050_demux.o
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= fc8050_hpi.o
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= fc8050_i2c.o
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= fc8050_isr.o
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= fc8050_ppi.o
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= fc8050_spi.o
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= fc8050_tun.o
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= fci_hal.o
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= fci_i2c.o
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= fci_oal.o
obj-$(CONFIG_LGE_BROADCAST_TDMB_FC8050)		+= fci_tun.o
